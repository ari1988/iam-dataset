          "arn:aws:lambda:*:*:function:*SageMaker*",
          "arn:aws:lambda:*:*:function:*sagemaker*",
          "arn:aws:lambda:*:*:function:*Sagemaker*"
        ],
        "Sid": "CustomLabelingJobs"
      },
      {
        "Action": [
          "s3:AbortMultipartUpload",
          "s3:GetObject",
          "s3:PutObject"
        ],
        "Effect": "Allow",
        "Resource": [
          "arn:aws:s3:::*GroundTruth*",
          "arn:aws:s3:::*Groundtruth*",
          "arn:aws:s3:::*groundtruth*",
          "arn:aws:s3:::*SageMaker*",
          "arn:aws:s3:::*Sagemaker*",
          "arn:aws:s3:::*sagemaker*"
        ]
      },
      {
        "Action": [
          "s3:GetObject"
        ],
        "Condition": {
          "StringEqualsIgnoreCase": {
            "s3:ExistingObjectTag/SageMaker": "true"
          }
        },
        "Effect": "Allow",
        "Resource": "*"
      },
      {
        "Action": [
          "s3:GetBucketLocation",
          "s3:ListBucket"
        ],
        "Effect": "Allow",
        "Resource": "*"
      },
